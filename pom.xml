        <module>widgets-db-sqlite</module>
        <module>widgets-db-redis</module>
        <module>widgets-db-mongo</module>
        <module>widgets-db-es</module>
        <module>widgets-qrcode</module>
        <module>widgets-mock</module>
        <module>widgets-javax-mail</module>
